                 &lt;span class=&#34;nx&#34;&gt;ng&lt;/span&gt;&lt;span class=&#34;o&#34;&gt;-&lt;/span&gt;&lt;span class=&#34;nx&#34;&gt;click&lt;/span&gt;&lt;span class=&#34;o&#34;&gt;=&lt;/span&gt;&lt;span class=&#34;s2&#34;&gt;&amp;#34;donotcopy.simulaterefineonclick = undefined;
&lt;/span&gt;&lt;span class=&#34;s2&#34;&gt;                           drawer.status = false&amp;#34;&lt;/span&gt;&lt;span class=&#34;o&#34;&gt;&amp;gt;&lt;/span&gt;
